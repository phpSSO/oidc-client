{
  "name": "juliuspc/openid-connect-php",
  "description": "OpenID Connect client",
  "keywords": [
    "OpenID Connect",
    "OIDC",
    "client",
    "OAuth 2",
    "SSO",
    "RFC6749",
    "RFC7009",
    "RFC7636",
    "RFC7662"
  ],
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.3",
    "phpseclib/phpseclib": "~2.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7",
    "guzzlehttp/psr7": "^1.8",
    "web-token/jwt-easy": "^2.2",
    "web-token/jwt-signature-algorithm-rsa": "^2.2",
    "web-token/jwt-signature-algorithm-hmac": "^2.2",
    "web-token/jwt-key-mgmt": "^2.2",
    "symfony/polyfill-php80": "^1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "archive": {
    "exclude": [
      ".*"
    ]
  },
  "autoload": {
    "psr-4": {
      "JuliusPC\\OpenIDConnect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JuliusPC\\OpenIDConnect\\Tests\\": "tests/"
    }
  }
}
